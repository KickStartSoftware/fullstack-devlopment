{
  "name": "desktop-app",
  "version": "1.0.0",
  "description": "Desktop api app.",
  "main": "dist/server.js",
  "scripts": {
    "ts:watch": "tsc --watch",
    "populate-categories": "node dist/populate-categories.js",
    "populate-db": "node dist/populate-db.js",
    "install-client": "cd client && yarn",
    "install-app": "yarn && yarn run install-client",
    "start-client": "cd client && yarn run dev",
    "dev-server": "nodemon dist/server.js --ignore client",
    "prod-server": "node dist/server.js --ignore client",
    "start-app-dev": "concurrently --kill-others-on-fail \"yarn run ts:watch\" \" yarn run dev-server\" \"yarn run start-client\"",
    "start-app-prod": "concurrently --kill-others-on-fail \"yarn run prod-server\" \"yarn run start-client\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "^7.4.6",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "chart.js": "^4.2.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.0.1",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^6.8.1",
    "morgan": "^1.10.0",
    "nodemailer": "^6.6.3",
    "rate-limiter": "^0.2.0",
    "recharts": "^2.5.0",
    "validator": "^13.7.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.15",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.17",
    "@types/validator": "^13.7.10",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20",
    "typescript": "^4.9.4"
  }
}
